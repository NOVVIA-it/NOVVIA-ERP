      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="Artikel" Background="#f5f5f5">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock x:Name="txtTest" Text="Artikel Detail Seite geladen!" FontSize="24"/>
            <TextBlock x:Name="txtStatus" Text="" FontSize="14" Margin="0,10,0,0"/>
            <Button Content="Zurueck" Click="Zurueck_Click" Margin="0,20,0,0" Padding="20,10"/>
        </StackPanel>
    </Grid>
</Page>
